{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "arrow-parens": "off",
      "comma-dangle": "off",
      "require-jsdoc": "off",
      "object-curly-spacing": "off",
      "operator-linebreak": "off",
      "no-underscore-dangle": [2, { "allow": ["_id"] }]
    }
}
